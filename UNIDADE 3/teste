#include <stdio.h>

int main()
{
    float num1, num2, soma, divisao,multi ;
    
    printf("Digite o primeiro numero:");
    scanf("%f",&num1);
    
      printf("Digite o segundo numero:");
    scanf("%f",&num2);
    
    soma = num1+num2;
    multi = num1*num2;
    divisao = num1/num2;
     printf("\nA soma desses numeros é: %.1f", soma);
     printf("\nA multiplicação desses numeros é: %.1f", multi);
     printf("\nA divisão desses numeros é: %.1f", divisao);
     
    
   
    return 0;
}
